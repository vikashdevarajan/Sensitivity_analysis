{
  "name": "strategix-game-theory-analyst",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "A professional full-stack application for zero-sum game theory analysis with AI-powered strategic advisory",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && python -m uvicorn main:app --host 0.0.0.0 --port 8000 --reload",
    "build": "cd frontend && npm run build",
    "preview": "cd frontend && npm run preview",
    "install:all": "npm run install:frontend && npm run install:backend",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && pip install -r requirements.txt",
    "start:backend": "cd backend && python -m uvicorn main:app --host 0.0.0.0 --port 8000",
    "start:frontend": "cd frontend && npm run preview"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "workspaces": [
    "frontend",
    "backend"
  ],
  "keywords": [
    "game-theory",
    "nash-equilibrium", 
    "strategic-analysis",
    "fastapi",
    "react",
    "typescript",
    "ai-advisory"
  ],
  "author": "Strategix Team",
  "license": "MIT"
}
